		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^5.0.0",
		"typescript": "^5.0.4",
		"vite": "^4.2.0"
	},
	"type": "module",
	"dependencies": {
		"@storyblok/svelte": "^3.0.0",
		"@vitejs/plugin-basic-ssl": "^1.0.1"
